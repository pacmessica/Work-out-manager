<div>
  <h1 id="title">Workout Manager</h1>
</div>
<div class="lower-header">
  <% if !user_signed_in? %>
    <%= link_to "Log In", new_user_session_path, class: "btn" %>
  <% else %>
    <span><%= current_user.email %></span>
    <%= link_to "Log Out", destroy_user_session_path, class: "btn", :method => :delete %>
  <% end %>
</div>
